{
    "name": "voyager-templates",
    "description": "Templating Voyager BREAD",
    "keywords": ["laravel", "voyager", "hooks"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Torrinha",
            "email": "bruno@torrinha.com"
        }
    ],
    "require": {
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^6.2|^5.6"
    },
    "autoload": {
        "psr-4": {
            "voyager-templates\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "voyager-templates\\Tests\\": "tests/"
        }
    },
    "extra": {
        "hook": {
            "providers": [
                "VoyagerTemplates\\VoyagerTemplatesServiceProvider"
            ],
            "aliases": {
                "VoyagerTemplates": "VoyagerTemplates\\VoyagerTemplatesFacade"
            },
            "migrations": [
                "database/migrations"
            ],
            "seeders": [
                "database/seeders"
            ]
        }
    }
}
